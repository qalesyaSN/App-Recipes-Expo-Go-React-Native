{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true, // Ini akan menghasilkan APK untuk development client
      "distribution": "internal", // Ini juga akan menghasilkan APK
      "android": {
        "buildType": "apk" // Eksplisit menghasilkan APK
      }
    },
    "preview": {
      "distribution": "internal", // Ini akan menghasilkan APK
      "android": {
        "buildType": "apk" // Eksplisit menghasilkan APK
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk" // Tambahkan ini untuk memastikan profil production menghasilkan APK
      }
    }
  },
  "submit": {
    "production": {}
  }
}
